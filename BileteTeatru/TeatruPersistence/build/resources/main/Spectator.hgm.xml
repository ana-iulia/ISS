<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="teatru.model.Spectator" table="Spectators">
        <id name="username" column="username">

        </id>
        <property
                name="password"
                column="password"/>
        <property
                name="name"
                column="name"/>
        <property
                name="phone"
                column="phone"/>
        <property
                name="email"
                column="email"/>


    </class>
</hibernate-mapping>